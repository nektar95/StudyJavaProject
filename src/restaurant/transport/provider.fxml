<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="640.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="restaurant.transport.ProviderController">
   <center>
      <ListView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="20.0" left="50.0" right="50.0" top="50.0" />
         </BorderPane.margin></ListView>
   </center>
   <bottom>
      <BorderPane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <left>
            <Button fx:id="buttonReturn" mnemonicParsing="false" onAction="#onReturnClicked" text="Return" BorderPane.alignment="CENTER_RIGHT">
               <BorderPane.margin>
                  <Insets left="150.0" />
               </BorderPane.margin>
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </left>
         <right>
            <Button fx:id="buttonDelete" mnemonicParsing="false" onAction="#onDeleteClicked" text="Delete" BorderPane.alignment="CENTER_LEFT">
               <font>
                  <Font size="20.0" />
               </font>
               <BorderPane.margin>
                  <Insets right="150.0" />
               </BorderPane.margin>
            </Button>
         </right>
      </BorderPane>
   </bottom>
</BorderPane>
